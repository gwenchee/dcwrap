# OAT Study 
# Varying Used Fuel Cooling Time 

environment, 
	tabular_data 
		tabular_data_file = 'cooling-time.dat'

method,						
	multidim_parameter_study
    partitions = 1

variables,					
	continuous_design = 1			
    lower_bounds      2     
    upper_bounds      3      
    descriptors       'ct'     

interface,							
	fork							
	  analysis_drivers = 'python cooling-time.py'

responses,				
	num_objective_functions = 1	
	no_gradients
	no_hessians